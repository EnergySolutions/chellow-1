<hibernate-mapping package="net.sf.chellow.physical">
	<class name="MpanCore" table="mpan_core">
		<id name="id">
			<generator class="sequence">
				<param name="sequence">mpan_core_id_sequence</param>
			</generator>
		</id>
		<many-to-one name="supply" class="Supply" column="supply_id"
			not-null="true" foreign-key="fkey_mpan_core_supply" />
		<many-to-one name="dso" class="net.sf.chellow.billing.Dso"
			column="dso_id" unique-key="mpan_core" not-null="true" foreign-key="fkey_mpan_core_dso" />
		<property type="string" name="uniquePart" column="unique_part"
			not-null="true" unique-key="mpan_core" />
		<property type="character" name="checkDigit" column="check_digit"
			not-null="true" unique-key="mpan_core" />
	</class>
</hibernate-mapping>