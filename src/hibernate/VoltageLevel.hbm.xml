<hibernate-mapping package="net.sf.chellow.physical">
	<class name="VoltageLevel" table="voltage_level" lazy="true">
		<id name="id">
			<generator class="sequence">
				<param name="sequence">voltage_level_id_sequence</param>
			</generator>
		</id>
		<component name="code" class="VoltageLevelCode">
			<property name="string" column="code" unique="true" />
		</component>
		<property name="name" column="name" unique="true" />
	</class>
</hibernate-mapping>

