${CHELLOW_HOME}/stop_chellow
dropdb chellow
createdb chellow
rm ${CHELLOW_HOME}/downloads/*
${CHELLOW_HOME}/start_chellow
if flake8 --filename=*t.py ${CHELLOW_HOME}/web/WEB-INF ; then
    . ${CHELLOW_HOME}/venv/bin/activate
    cd ${CHELLOW_HOME}/test
    imprimatur ${CHELLOW_HOME}/test/imprimatur.py  || { exit 1; }
fi
